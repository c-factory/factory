{
    "name": "c factory",
    "description": "The build system for C programs",
    "author": "c factory",
    "type": "application",
    "sources": ["test/*.c", "test/src/subroutine.c"],
    "headers": ["test/src"],
    "depends":
    [
        {
            "name": "strings",
            "path": "../strings",
            "sources": "strings.c",
            "headers": ".",
            "depends":
            [
                {
                    "name": "collections",
                    "path": "../collections",
                    "sources": "src/*.c",
                    "headers": "include"
                }
            ]
        }
    ]
}
